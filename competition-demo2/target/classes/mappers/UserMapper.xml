<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.competitiondemo2.mapper.UserMapper">
  <resultMap id="BaseResultMap" type="com.example.competitiondemo2.pojo.User">
    <result column="uid" jdbcType="INTEGER" property="uid" />
    <result column="uname" jdbcType="VARCHAR" property="uname" />
    <result column="password" jdbcType="VARCHAR" property="password" />
  </resultMap>
  <insert id="insert" parameterType="com.example.competitiondemo2.pojo.User">
    insert into user (uid, uname, password
      )
    values (#{uid,jdbcType=INTEGER}, #{uname,jdbcType=VARCHAR}, #{password,jdbcType=VARCHAR}
      )
  </insert>

  <select id="queryAll" resultType="User">
    select * from user
  </select>

  <select id="selectByName" resultType="User">
    select * from user where uname = #{uname,jdbcType=VARCHAR} and password = #{password,jdbcType=VARCHAR}
  </select>


</mapper>